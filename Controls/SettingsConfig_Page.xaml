<UserControl x:Class="WatchedAnimeList.Controls.SettingsConfig_Page"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WatchedAnimeList.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1920"
             HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

    <Grid Background="Transparent">
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <TextBlock x:Name="PageHistory_TextBlock" Foreground="Gray" FontSize="12" Margin="20,0" VerticalAlignment="Center"/>

        </Grid>
    </Grid>
</UserControl>
